public class Main
{
	public static void main(String[] args) {
		int max_sum = Integer.MIN_VALUE;
		int[] arr = {-4,-3,-6,-9,2};
		for(int i=0;i<arr.length;i++){
		    int sum = 0;
		    for(int j = 0;j<arr.length;j++){
		        //Internal sum
            for(int k = i;k<=j;k++){
		            sum = sum + arr[k];
		        }
		        max_sum = Math.max(sum,max_sum);
		    }
		}
		System.out.print(max_sum);
	}
}
TC : O(n^3)
SC : ON(1)
